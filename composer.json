{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "aelvan/inlin": "^2.1",
    "besteadfast/craft-preparse-field": "1.4.1",
    "carlcs/craft-redactorcustomstyles": "^3.0",
    "craftcms/cms": "3.7.67",
    "craftcms/contact-form": "2.5.1",
    "craftcms/element-api": "2.8.6.1",
    "craftcms/feed-me": "4.5.4",
    "craftcms/google-cloud": "1.4.1",
    "craftcms/redactor": "2.10.11",
    "doublesecretagency/craft-inventory": "^2.1",
    "dukt/twitter": "2.1.6",
    "dukt/videos": "2.1.0",
    "guzzlehttp/guzzle": "^7.0",
    "lewisjenkins/craft-dynamic-fields": "3.0.12",
    "mark-wff/craft-var-dumper": "v1.3.0",
    "mediabeastnz/timeago": "1.0.3",
    "mildlygeeky/craft-redactor-script-buttons": "^1.1",
    "mmikkel/incognito-field": "1.3.0",
    "nystudio107/craft-retour": "3.2.8",
    "nystudio107/craft-seomatic": "3.4.49",
    "putyourlightson/craft-blitz": "3.12.8",
    "putyourlightson/craft-sendgrid": "1.3.1",
    "samhernandez/craft-shortcodes": "1.1.0",
    "sebastianlenz/linkfield": "1.0.25",
    "sgtpenguin/csv-reader": "1.0.0",
    "sgtpenguin/geocode": "1.0.0",
    "sgtpenguin/special-relations": "^1.0",
    "solspace/craft-freeform": "3.13.24",
    "spacecatninja/imager-x": "v3.6.5",
    "spicyweb/craft-neo": "2.13.16",
    "topshelfcraft/supersort": "3.1.0",
    "venveo/craft-bulkedit": "^2.0",
    "verbb/color-mixer": "2.0.1",
    "verbb/many-to-many": "2.0.2",
    "verbb/shortcodes": "^2.0",
    "vlucas/phpdotenv": "^3.4.0",
    "wrav/oembed": "^1.3"
  },
  "repositories": [
    {
      "type": "path",
      "url": "plugins/specialrelations"
    },
    {
      "type": "path",
      "url": "plugins/geocode"
    },
    {
      "type": "path",
      "url": "plugins/csvreader"
    }
  ],
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "modules\\shortcodesmodule\\": "modules/shortcodesmodule/src"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4.27"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
