{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "aelvan/inlin": "^2.1",
    "besteadfast/craft-preparse-field": "1.2.5",
    "carlcs/craft-redactorcustomstyles": "^3.0",
    "craftcms/cms": "3.7.34",
    "craftcms/contact-form": "2.3.0",
    "craftcms/element-api": "2.8.4",
    "craftcms/feed-me": "^4.3",
    "craftcms/google-cloud": "1.4.1",
    "craftcms/redactor": "2.10.5",
    "doublesecretagency/craft-inventory": "^2.1",
    "dukt/twitter": "^2.1",
    "dukt/videos": "2.0.15",
    "lewisjenkins/craft-dynamic-fields": "3.0.12",
    "mediabeastnz/timeago": "1.0.3",
    "mildlygeeky/craft-redactor-script-buttons": "^1.1",
    "mmikkel/incognito-field": "1.2.0",
    "nystudio107/craft-retour": "3.1.70",
    "nystudio107/craft-seomatic": "3.4.26",
    "page-8/craft-manytomany": "1.0.2.2",
    "putyourlightson/craft-blitz": "3.11.1",
    "putyourlightson/craft-sendgrid": "1.2.3",
    "samhernandez/craft-shortcodes": "1.1.0",
    "sebastianlenz/linkfield": "1.0.25",
    "sgtpenguin/csv-reader": "1.0.0",
    "sgtpenguin/geocode": "1.0.0",
    "sgtpenguin/special-relations": "^1.0",
    "solspace/craft-freeform": "3.13.1",
    "spacecatninja/imager-x": "v3.5.5",
    "spicyweb/craft-neo": "2.12.5",
    "topshelfcraft/supersort": "3.1.0",
    "vlucas/phpdotenv": "^3.4.0",
    "whoisjuan/craft-color-mixer": "1.0.9",
    "wrav/oembed": "^1.3"
  },
  "repositories": [
    {
      "type": "path",
      "url": "plugins/specialrelations"
    },
    {
      "type": "path",
      "url": "plugins/geocode"
    },
    {
      "type": "path",
      "url": "plugins/csvreader"
    }
  ],
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "modules\\shortcodesmodule\\": "modules/shortcodesmodule/src"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4.27"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
