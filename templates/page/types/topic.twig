{% extends '_layout' %}
{% set title = entry.title %}
{% block content %}

{% set blocks =  entry.topicPageBlocks %}
<div class="topicPage">
	{% for block in blocks %}
		{% if block.type == 'copy' %}
    	{% include 'v2/common/copy' with { block: block }  %}
		{% elseif block.type == 'quote' %}
    	{% include 'v2/common/quote' with { block: block } %}
		{% elseif block.type == 'stats' %}
    	{% include 'v2/common/stats' with { block: block } %}
		{% elseif block.type == 'listing' %}
    	{% include 'v2/common/listing' with { block: block } %}
		{% elseif block.type == 'exploreMore' %}
    	{% include 'v2/common/exploreMore' with { block: block } %}	
		{% elseif block.type == 'signUp' %}
    	{% include 'v2/common/signUp' with { block: block } %}
		{% elseif block.type == 'newsHero' %}
    	{% include 'v2/common/newsHero' with { block: block } %}			
		{% elseif block.type == 'newsItems' %}
    	{% include 'v2/common/newsItems' with { block: block } %}
		{% elseif block.type == 'heroType1' %}
    	{% include 'v2/common/heroType1' with { block: block } %}		
		{% elseif block.type == 'bannerType3' %}
			{% include 'v2/common/bannerType3' with { block: block } %}	   
		{#
		{% elseif block.type == 'ctaBanner' %}
    	{% include 'includes/topic-blocks/ctaBanner'  with { ctaBannerBlock: block } %}	   
		#}
		{% endif %}
	{% endfor %}
</div>
{% endblock %}

